{
  "name": "image-picker-plugin",
  "version": "0.0.0",
  "description": "Image file picker plugin for Milkdown",
  "author": "Ayaka Rizumu<https://github.com/LittleSound>",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@milkdown/core": "*",
    "@milkdown/prose": "*",
    "@milkdown/preset-commonmark": "^6.3.1"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  }
}
