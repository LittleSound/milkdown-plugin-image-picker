{
  "name": "ts-lib-starter",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.7.0",
  "description": "boilerplate for your next TypeScript library",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "17.0.38",
    "@milkdown/core": "^6.3.1",
    "dprint": "0.29.1",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "tsup": "6.0.1",
    "typescript": "4.7.2",
    "vite": "2.9.9",
    "vitest": "0.13.1"
  },
  "peerDependencies": {
    "@milkdown/core": "*"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  }
}
